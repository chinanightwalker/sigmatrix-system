package com.sigmatrix.sc.web.vo;


/**
  * 过渡页管理添加或修改操作VO接收类
  * 参数为页面输入参数 
  * @author zyc 作者 E-mail: 
  * @String 创建时间：2016年7月21日 下午2:47:53
  * @version 1.0
  * @parameter
  * @since
  * @return
  */
public class ScancodeTransitionVo {
 	private Integer transitionId;

    private Integer productId;

    private String startTime;

    private String endTime;

    private Integer isOpen;

    private String picUrl;

    private String picPath;

    
	public ScancodeTransitionVo() {
		super();
		// TODO Auto-generated constructor stub
	}

	public ScancodeTransitionVo(Integer transitionId, Integer productId, String startTime, String endTime, Integer isOpen,
			String picUrl, String picPath) {
		super();
		this.transitionId = transitionId;
		this.productId = productId;
		this.startTime = startTime;
		this.endTime = endTime;
		this.isOpen = isOpen;
		this.picUrl = picUrl;
		this.picPath = picPath;
	}

	public Integer getTransitionId() {
		return transitionId;
	}

	public void setTransitionId(Integer transitionId) {
		this.transitionId = transitionId;
	}

	public Integer getProductId() {
		return productId;
	}

	public void setProductId(Integer productId) {
		this.productId = productId;
	}

	public String getStartTime() {
		return startTime;
	}

	public void setStartTime(String startTime) {
		this.startTime = startTime;
	}

	public String getEndTime() {
		return endTime;
	}

	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}

	public Integer getIsOpen() {
		return isOpen;
	}

	public void setIsOpen(Integer isOpen) {
		this.isOpen = isOpen;
	}

	public String getPicUrl() {
		return picUrl;
	}

	public void setPicUrl(String picUrl) {
		this.picUrl = picUrl;
	}

	public String getPicPath() {
		return picPath;
	}

	public void setPicPath(String picPath) {
		this.picPath = picPath;
	}
    
}
