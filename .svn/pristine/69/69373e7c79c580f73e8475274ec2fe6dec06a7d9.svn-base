body {
	font-family: '微软雅黑';
}

body,div,table,tr,td,img,form{
	margin: 0px;
	padding: 0px;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
img {
	border: none;
}
a {
	text-decoration: none;
}
.pro_mask{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 200;
	background: url("<%=request.getContextPath()%>/images/activity/mask_bg.png") repeat;
}

.pro_mask_div{
	position: relative;
	left: 25%;
	top: 10px;
	width: 50% !important;
	background: #fff;
	z-index: 201;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	padding: 10px 0px;
}
.addPrize_mask{
	position: relative;
	background: #fff;
	z-index: 201;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	width: 50%;
	top: 20px;
	padding: 10px;
	margin: 0px auto;
	height: 70%;
}
ul,li {list-style-type:none;margin: 0px;padding: 0px; }
#nav {display: block;}
#nav .mainlevel {float:left;text-align:center; display:block;}
#nav .mainlevel a {font-size: 14px;color: #434343; text-decoration:none;text-align:center; display:block;}
#nav .mainlevel ul {position:absolute;display: none;}
#nav .mainlevel li {float:left;}


.zhezhaoceng{
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:100%;
	background-color:rgba(0,0,0,.7);
}
