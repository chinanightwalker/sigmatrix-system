package com.sigmatrix.sc.utils;

public class PrizeConfig {
	
	private int prizeId; //奖项ID
	private int hasLotteryCount; //已经抽过奖的数量
	private int hasPrizeCount;  //已经中了此奖项的数量,如果是按箱抽奖，则是此箱中中了此奖的数量
	private Float prizeRate;  //中奖率 百万分之几
	private int prizeCount; //此中奖的数量，即投放量
	public int getPrizeId() {
		return prizeId;
	}
	public int getHasLotteryCount() {
		return hasLotteryCount;
	}
	public void setHasLotteryCount(int hasLotteryCount) {
		this.hasLotteryCount = hasLotteryCount;
	}
	public void setPrizeId(int prizeId) {
		this.prizeId = prizeId;
	}

	public int getHasPrizeCount() {
		return hasPrizeCount;
	}
	public void setHasPrizeCount(int hasPrizeCount) {
		this.hasPrizeCount = hasPrizeCount;
	}
	
	public int getPrizeCount() {
		return prizeCount;
	}
	public void setPrizeCount(int prizeCount) {
		this.prizeCount = prizeCount;
	}
	public Float getPrizeRate() {
		return prizeRate;
	}
	public void setPrizeRate(Float prizeRate) {
		this.prizeRate = prizeRate;
	}
	
	public String toString(){
		StringBuilder sb = new StringBuilder();
		sb.append("prizeId:"+this.prizeId + "\t" );
		sb.append("hasLotteryCount:"+this.hasLotteryCount+ "\t" );
		sb.append("hasPrizeCount:"+ this.hasPrizeCount);
		sb.append("prizeRate:" + this.prizeRate+ "\t" );
		sb.append("prizeCount:" + this.prizeCount);
		return sb.toString();
	}
	
	

}
